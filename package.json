{
  "name": "server",
  "version": "1.0.0",
  "description": "This is an Andela Bootcamp 30 project. WeConnect is a business listing and out sourcing web application.",
  "main": "app.js",
  "scripts": {
    "test": "set NODE_ENV=test&&npm run test-migrate && nyc --reporter=lcov --reporter=text mocha --timeout 5000 --exit --compilers js:babel-core/register server/specs/*test.js || true",
    "test-migrate": "sequelize db:migrate:undo:all --env test && sequelize db:migrate --env test",
    "build": "babel server -d server/dist",
    "unbuild": "rm -rf node_modules || rd /s /q node_modules",
    "rebuild": "npm run unbuild && npm install",
    "postinstall": "npm run build",
    "start": "babel-node app.js",
    "start:dev": "nodemon --watch server --exec babel-node -- app.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "showcoverage": "start coverage/lcov-report/index.html",
    "migrate": "sequelize db:migrate:undo:all && sequelize db:migrate",
    "prod-migrate": "sequelize db:migrate:undo:all --env production && sequelize db:migrate --env production",
    "lint": "eslint --fix server/",
    "webpack": "webpack-dev-server -d --config ./webpack.config.dev.js --hot"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PascalUlor/WeConnect.git"
  },
  "author": "Pascal Ulor",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PascalUlor/WeConnect/issues"
  },
  "homepage": "https://github.com/PascalUlor/WeConnect#readme",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-react": "^1.1.7",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-require-jsdoc-except": "^1.1.0",
    "file-loader": "^1.1.11",
    "istanbul": "^0.4.5",
    "node-sass": "^4.9.2",
    "nodemon": "^1.18.3",
    "sass-loader": "^7.1.0",
    "sequelize-cli": "^4.0.0",
    "style-loader": "^0.21.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {
    "bcrypt": "^3.0.2",
    "body-parser": "^1.18.3",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "cors": "^2.8.4",
    "coveralls": "^3.0.2",
    "dotenv": "^5.0.1",
